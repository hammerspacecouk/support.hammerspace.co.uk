{
  "name": "hammerspace/support.hamemrspace.co.uk",
  "license": "MIT",
  "type": "project",
  "description": "Support hammerspace",
  "autoload": {
    "psr-4": {
      "AppBundle\\": "src/AppBundle/",
      "SupportService\\": "src/SupportService/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\AppBundle\\": "tests/AppBundle/",
      "Tests\\SupportService\\": "tests/SupportService/"
    }
  },
  "require": {
    "php": "^7.1",
    "symfony/symfony": "^3.2",
    "symfony/monolog-bundle": "^3.0",
    "doctrine/orm": "^2.5.6",
    "doctrine/doctrine-bundle": "^1.6",
    "doctrine/migrations": "^1.5",
    "doctrine/doctrine-migrations-bundle": "^1.2",
    "stripe/stripe-php": "4.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "squizlabs/php_codesniffer": "^2.7",
    "escapestudios/symfony2-coding-standard": "^2.9",
    "doctrine/data-fixtures": "^1.2"
  },
  "config": {
    "bin-dir": "vendor/bin",
    "platform": {
      "php" : "7.1"
    }
  },
  "extra": {
    "symfony-app-dir": "app",
    "symfony-web-dir": "public",
    "symfony-assets-install": "relative",
    "incenteev-parameters": {
      "file": "app/config/parameters.yml"
    }
  }
}

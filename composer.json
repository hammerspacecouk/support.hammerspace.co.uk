{
  "name": "hammerspace/support.hamemrspace.co.uk",
  "license": "MIT",
  "type": "project",
  "description": "Support hammerspace",
  "autoload": {
    "psr-4": {
      "": "src/"
    }
  },
  "require": {
    "php": "~7.1",
    "symfony/symfony": "~3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~5",
    "squizlabs/php_codesniffer": "~2",
    "escapestudios/symfony2-coding-standard": "~2.0"
  },
  "config": {
    "bin-dir": "vendor/bin",
    "platform": {
      "php" : "7.1"
    }
  },
  "extra": {
    "symfony-app-dir": "app",
    "symfony-web-dir": "public",
    "symfony-assets-install": "relative",
    "incenteev-parameters": {
      "file": "app/config/parameters.yml"
    }
  }
}
